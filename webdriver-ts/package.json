    "jstat": "1.9.4",
    "lighthouse": "7.5.0",
    "lodash": "4.17.21",
    "marky": "^1.2.2",
    "npm-check-updates": "11.5.13",
    "ramda": "0.27.1",
    "selenium-webdriver": "4.0.0-beta.3",
    "semver": "7.3.5",
    "yargs": "17.0.1"
  }
