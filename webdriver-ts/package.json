    "@types/selenium-webdriver": "4.0.12",
    "@types/semver": "7.3.6",
    "@types/yargs": "17.0.0",
    "dockerode": "^3.3.0",
    "rimraf": "^3.0.2",
    "ts-node": "10.0.0",
    "typescript": "4.3.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "chromedriver": "^90.0.1",
    "cross-env": "7.0.3",
